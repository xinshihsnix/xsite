<uwsgi>
  <socket>0.0.0.0:9527</socket>
  <listen>64</listen>
  <master>true</master>
  <pidfile>/usr/local/nginx/xsite_uwsgi.pid</pidfile>
  <processes>2</processes>
  <module>wsgi</module>
  <pythonpath>/var/projects/xsite/</pythonpath>
  <profiler>true</profiler>
  <memory-report>true</memory-report>
  <enable-threads>true</enable-threads>
  <logdate>true</logdate>
  <logto>/var/log/uwsgi/xsite_uwsgi.log</logto>
  <limit-as>6048</limit-as>
</uwsgi>
